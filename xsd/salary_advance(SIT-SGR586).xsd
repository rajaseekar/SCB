<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <!-- Common element start -->
    <xs:element name="eform">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="model">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="instance" maxOccurs="2"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="FID" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="instance">
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:group ref="savemodel" id="savemodel"/>
                    <xs:group ref="outputmodel" id="outputmodel"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="id">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="savemodel|outputmodel"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:group name="savemodel">
        <xs:sequence>
            <xs:element name="sigfields">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element ref="sigchar"/>
                        <xs:element ref="signum"/>
                        <xs:element ref="sigdate"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
